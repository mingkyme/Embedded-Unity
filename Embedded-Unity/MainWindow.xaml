<Window x:Class="Embedded_Unity.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Embedded_Unity"
        mc:Ignorable="d"
        Loaded="Window_Loaded"
        Title="EmbeddedUnity" Height="450" Width="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="1*"/>
            <RowDefinition Height="3*"/>
        </Grid.RowDefinitions>
        <TextBlock Text="WPF Program" FontSize="50" VerticalAlignment="Center" HorizontalAlignment="Center"/>
        <Grid x:Name="XAML_Unity_Parent" Grid.Row="1"/>
    </Grid>
</Window>
